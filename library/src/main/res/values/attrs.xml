<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KeyboardView">
        <attr name="style" format="integer">
            <enum name="number" value="0" />
            <enum name="discountNumber" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ProgressView">
        <attr name="defualtText" format="string" />
        <attr name="originColor" format="color" />
        <attr name="finishColor" format="color" />
        <attr name="errorColor" format="color" />
    </declare-styleable>
    <declare-styleable name="ClearEditText">
        <attr name="clearIcon" format="reference" />
        <attr name="scaleSize" format="float" />
        <attr name="drawableWidth" format="dimension" />
    </declare-styleable>
</resources>